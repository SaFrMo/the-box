{
  "name": "the-dom-box",
  "version": "1.1.0",
  "description": "Utilities for working with DOM boxes.",
  "main": "lib/index.js",
  "scripts": {
    "cleanup": "rimraf temp lib",
    "prebuild": "npm run test && npm run cleanup",
    "build": "babel src --out-dir lib --ignore .spec.js",
    "test": "karma start karma.conf.js --single-run",
    "dev": "karma start karma.conf.js",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "postversion": "git push && git push --tags",
    "version": "npm run build && npm run changelog && git add -A"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/fczbkk/the-box.git"
  },
  "keywords": [
    "dom",
    "box",
    "size",
    "bounding",
    "collection",
    "viewport",
    "document"
  ],
  "author": {
    "name": "Riki Fridrich",
    "email": "riki@fczbkk.com",
    "url": "http://fczbkk.com/"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/fczbkk/the-box/issues"
  },
  "homepage": "https://github.com/fczbkk/the-box#readme",
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-loader": "^6.2.3",
    "babel-preset-es2015": "^6.5.0",
    "conventional-changelog-cli": "^1.1.1",
    "jasmine-core": "^2.4.1",
    "karma": "^0.13.21",
    "karma-coverage": "^0.5.3",
    "karma-jasmine": "^0.3.7",
    "karma-mocha-reporter": "^1.2.0",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-webpack": "^1.7.0",
    "phantomjs-prebuilt": "^2.1.4",
    "rimraf": "^2.5.2",
    "webpack": "^1.12.13"
  },
  "dependencies": {
    "iselement": "^1.0.0"
  }
}
